    "def longestWord(word_list):\n",
    "    total_words = []\n",
    "    for  i in word_list:\n",
    "        total_words.append([len(i),i])\n",
    "    total_words.sort()\n",
    "    return total_words[-1][1]\n",
    "\n",
    "longestWord(['adfsdf','skuhsifhsdf','dfd','hjfhfds'])"
